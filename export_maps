/**** Start of imports. If edited, may not auto-convert in the playground. ****/
var cancook_htv2_geo = ee.Image("users/mitchest/gbr_reefs/cancook_dove_htv2_benthic-clean1"),
    cancook_htv2_benthic = ee.Image("users/mitchest/gbr_reefs/cancook_dove_htv2_geo-clean2");
/***** End of imports. If edited, may not auto-convert in the playground. *****/

// Cancook export
var export_convhull = cancook_htv2_geo.gte(0).reduceToVectors({scale: 1000, maxPixels: 1e13, bestEffort: true, geometry: cancook_htv2_geo.geometry().bounds()}).geometry().convexHull({maxError: 100})

// ######
// Need to add raw classification, make a mask, and update mask for export
// ######


Export.image.toDrive({
  image: cancook_htv2_geo, 
  description: 'cancook_geo', 
  region: export_convhull,
  scale: 25,
  maxPixels: 1e13
})

Export.image.toDrive({
  image: cancook_htv2_benthic, 
  description: 'cancook_benthic', 
  region: export_convhull,
  scale: 25,
  maxPixels: 1e13
})

Map.addLayer(export_convhull)