var outline1 = ee.Image("projects/coral_atlas/reefMask/maskOut/reefMask_gen3_S30_v7_gridID_a"),
    reef_boundary = 
    /* color: #d63000 */
    /* shown: false */
    ee.Geometry.Polygon(
        [[[145.07440257760027, -14.691627878507992],
          [145.11834789010027, -14.919991125309087],
          [145.18426585885027, -15.381283949256629],
          [145.33258128853777, -15.957783590613852],
          [145.37652660103777, -16.332415505310827],
          [146.15655589791277, -17.205494569435665],
          [146.21148753853777, -17.598624119336375],
