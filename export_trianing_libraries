var aca_benthic_train = [
  "projects/coral_atlas/Micronesia/training/wmic_benthic_training",
  "projects/coral_atlas/east_africa/training/east_africa_benthic_training",
  "projects/coral_atlas/hawaii/training/hawaii_benthic_training",
  "projects/coral_atlas/nth_cari/training/nth_cari_benthic_training",
  "projects/coral_atlas/sw_pacific/training/swp_benthic_training",
  "projects/coral_atlas/west_indian_ocean/training/wio_benthic_training",
