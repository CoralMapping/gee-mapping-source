var wcmc = ee.FeatureCollection("projects/coral_atlas/global_datasets/wcmc_reefs_2018v4_dissolved"),
    imageVisParam = {"opacity":1,"bands":["b3","b2","b1"],"max":1536,"gamma":1},
    low_rrs = ee.Image("projects/coral_atlas/workflow/mapping/west_indian_ocean/west_indian_ocean_islands_low_tide_normalized_sr_jan2018_jan2020_v2_shift_mosaic/20200131T210330Z/west_indian_ocean_islands_low_tide_normalized_sr_jan2018_jan2020_v2_shift_mosaic"),
    extent_to_wcmc = 
    /* color: #ff0000 */
    /* shown: false */
    /* locked: true */
    ee.Geometry.Polygon(
        [[[42.68712638818556, -10.460493882645412],
          [41.63243888818556, -12.78471059058158],
          [44.18126701318556, -14.32263202370624],
