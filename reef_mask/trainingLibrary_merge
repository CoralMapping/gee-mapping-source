/**** Start of imports. If edited, may not auto-convert in the playground. ****/
var table = ee.FeatureCollection("projects/coral_atlas/reefMask/covariateData/reefMaskCovariateData_v8_grid_0"),
    table2 = ee.FeatureCollection("projects/coral_atlas/reefMask/covariateData/reefMaskCovariateData_v8_grid_1"),
    table3 = ee.FeatureCollection("projects/coral_atlas/reefMask/covariateData/reefMaskCovariateData_v8_grid_2");
/***** End of imports. If edited, may not auto-convert in the playground. *****/

var reefMaskTrainingSet = table.merge(table2.merge(table3.merge(table4.merge(table5.merge(table6.merge(table7.merge(table8.merge(table9.merge(table10.merge(table11.merge))))))))))
print (reefMaskTrainingSet.size());
print (reefMaskTrainingSet.filterMetadata('CLASS', 'equals', 1).size(), 'land');
print (reefMaskTrainingSet.filterMetadata('CLASS', 'equals', 2).size(), 'water');
print (reefMaskTrainingSet.filterMetadata('CLASS', 'equals', 3).size(), 'reef') ;












